{
  "name": "eduhu-test",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "CI=true jest --coverage --ci --verbose && playwright test",
    "test:unit": "jest --testPathPattern=src/ --verbose",
    "test:integration": "jest --testPathPattern=tests/integration/ --verbose",
    "qa:check": "npm run lint && npm run test:coverage && npm run test:e2e",
    "qa:teacher-workflows": "playwright test --grep='teacher workflow'",
    "qa:performance": "playwright test --grep='performance'",
    "qa:accessibility": "playwright test --grep='accessibility'",
    "qa:security": "npm audit --audit-level high && npm run test:security",
    "qa:mobile": "playwright test --grep='mobile|responsive|PWA'",
    "qa:full": "npm run qa:check && npm run qa:performance && npm run qa:accessibility && npm run qa:security && npm run qa:mobile",
    "test:security": "jest --testPathPattern=tests/security/",
    "test:performance": "playwright test tests/performance/",
    "test:chat-sessions": "playwright test tests/integration/chat-sessions.test.ts",
    "test:file-upload": "playwright test tests/performance/file-upload.test.ts",
    "db:migrate:test": "echo 'Test database migration placeholder'",
    "generate:test-fixtures": "node scripts/generate-test-fixtures.js",
    "qa:report": "node scripts/generate-qa-report.js",
    "validate:ci-artifacts": "node scripts/validate-ci-artifacts.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.698.0",
    "@aws-sdk/s3-request-presigner": "^3.698.0",
    "@headlessui/react": "^2.2.8",
    "@instantdb/core": "^0.21.23",
    "@instantdb/react": "^0.21.23",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "mammoth": "^1.8.0",
    "next": "^15.5.4",
    "openai": "^5.23.0",
    "pdf-parse": "^1.1.1",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.49.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "axe-core": "^4.10.2",
    "axe-playwright": "^2.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.21",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lighthouse": "^12.2.1"
  }
}
